    min-height: 50px;
    padding: 0px;
}

#navigation_toolbar{
    float: right;
}

#navigation_toolbar table {
    color: slategray;
}

#navigation {
    position: relative;
    z-index: 9999;
}

input[type='text'].search {
    border:1px solid #4F81BD;
    min-height: 17px;
    border-radius: 2px;
}

a.logout {
    color: slategray;
    text-decoration: none;
    font-size: 14px;
}

input:focus,
select:focus,
textarea:focus,
button:focus {
    outline: none;
}

a.disabled {
    background-color:slategray!important;
    color: white;
    text-decoration: none;
    cursor: default!important;
}
a.disabled:hover {
    text-decoration: none!important;
}

/*
